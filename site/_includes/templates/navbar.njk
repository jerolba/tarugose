{% import "components.njk" as comp %}

<nav class="navbar">
    <strong class="navbar-logo">
        Tarugose
    </strong>

    <ul class="navbar-menu">
        {% for link in menu %}
        <li>
            {{ comp.link(link) | safe }}
        </li>
        {% endfor %}
    </ul>

    <ul class="navbar-buttons buttonGroup">
        {% for link in ctas %}
        <li>
            {{ comp.link(link, 'button') | safe }}
        </li>
        {% endfor %}
    </ul>
</nav>
